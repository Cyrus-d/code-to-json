        95
      ],
      "statements": [
        80,
        95
      ]
    },
    "require": [
      "ts-node/register",
      "source-map-support/register"
    ],
    "extension": [
      ".ts"
    ],
    "include": [
      "src"
    ],
    "reporter": [
      "lcov",
      "json",
      "text-summary"
    ]
