  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch CLI",
      "preLaunchTask": "build",
      "timeout": 500,
      "runtimeArgs": ["-r", "ts-node/register"],
      "env": {
        "DEBUG": "code-to-json*"
      },
      "program": "${workspaceFolder}/packages/cli/lib/index.js",
      "args": [
        "-c",
        "./samples/js-single-file",
        "-o",
        "./out.json",
        "./samples/js-single-file/src/index.js"
      ],
      "console": "internalConsole",
      "internalConsoleOptions": "openOnFirstSessionStart"
    }
  ]
}
